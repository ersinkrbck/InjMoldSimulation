FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid 0
);

regions
(
    boxToCell
    {
        box (-0.5 -0.5 -10.5) (0 4.5 10.5);
        fieldValues
        (
			volScalarFieldValue T 493.15
            volScalarFieldValue alpha.liquid 1
        );
    }
);


